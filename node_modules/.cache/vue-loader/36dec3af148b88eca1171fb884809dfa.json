     checkPass: [\n            { validator: validatePass2, trigger: \"blur\", require: true },\n            ,\n          ],\n          email: [{ validator: validateEmail, trigger: \"blur\", require: true }],\n        },\n      },\n    };\n  },\n  methods: {\n    registerChange() {\n      this.status = !this.status;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.register {\n  width: 100%;\n  height: 775px;\n  background-color: #eee;\n}\n.register_box {\n  width: 480px;\n  height: 470px;\n  background-color: #fff;\n  border-radius: 3px;\n  position: absolute;\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n  box-shadow: 0 0 20px #ddd;\n}\n.tagMenu {\n  width: 480px;\n  height: 55px;\n  background-color: #eee;\n}\n.mobile {\n  width: 50%;\n  display: inline-block;\n  text-align: center;\n  font-size: 20px;\n  font-weight: bold;\n  color: #313131;\n  cursor: pointer;\n  line-height: 55px;\n}\n.email {\n  width: 50%;\n  display: inline-block;\n  text-align: center;\n  font-size: 20px;\n  font-weight: bold;\n  color: #313131;\n  cursor: pointer;\n  line-height: 55px;\n}\n.email_form {\n  position: absolute;\n  top: 80px;\n  width: 100%;\n  box-sizing: border-box;\n}\n.el-form-item >>> .el-form-item__error {\n  left: 30px;\n}\n.el-input {\n  width: 420px;\n  margin: 0 30px;\n}\n.active {\n  background-color: #fff;\n}\n/*btnFrame*/\n.btnFrame {\n  position: absolute;\n  bottom: 60px;\n}\n.tips {\n  margin: 0 30px;\n}\n.regBtn {\n  width: 420px;\n  height: 45px;\n  background-color: coral;\n  color: #fff;\n  cursor: pointer;\n  border: 0;\n  border-radius: 3px;\n  margin: 0 30px;\n}\n.tips_agreement {\n  font-size: 14px;\n  color: coral;\n  line-height: 14px;\n  white-space: nowrap;\n}\n.tips_login {\n  color: coral;\n}\n</style>"]}]}